@article{ref3,
   abstract = {Se estudiaron los ensilados de subproductos de alcachofa (EA) y maíz dulce (EMD), procedentes de la industria de conservas vegetales y localizados en siete explotaciones ganaderas de vacuno de leche de la región de Murcia, España. Se analizaron sus cualidades nutritivas, capacidad de fermentación y respuesta a los procesos de fermentación in vitro, con especial atención a la cinética de la digestión y al ajuste a ecuación descrita por McDonald (1981): y = b (1 - e<sup>-c (t-L)</sup>). Según los resultados, el ensilado del subproducto de alcachofa tuvo una fermentación de tipo acético, mientras que el de maíz dulce tuvo una fermentación de tipo láctica. La digestibilidad de la materia orgánica fue de 44.9 y 51.3 % para el EA y EMD, respectivamente. La desaparición in vitro de MS fue de 63.6 y de 74.5 % para los EA y EMD, respectivamente. La desaparición de FDN fue de 53.0 y 71.9 %, para ambos ensilados. Los datos de la producción de gas a las 72 h para los subproductos de EA y EMD, fueron de 94.4 y 124.5 mL 500 g<sup>-1</sup>, respectivamente. Una vez estudiadas todas las características, se concluye que el EMD tiene alta calidad nutritiva, facilidad para la fermentación y alta degradación ruminal. Se debería mejorar la aptitud a la fermentación del EA con la aplicación de aditivos.},
   author = {A Martínez-Teruel and Fuensanta Hernández and Josefa Madrid and María D Megías},
   issn = {0034-7485},
   issue = {1},
   journal = {Revista Cubana de Ciencia Agricola},
   month = {2},
   pages = {43-47},
   publisher = {Instituto de Ciencia Animal},
   title = {Valor nutritivo in vitro y ensilabilidad de los ensilados de los subproductos agroindustriales de alcachofa y maíz dulce.},
   volume = {41},
   year = {2007},
}
